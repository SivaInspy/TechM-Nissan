<bundle-app name='EQUIP-App-bundl' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance' xmlns='uri:oozie:bundle:0.2'>
	<parameters>
		<property>
			<name>techlineCoord</name>
			<value>${techlineWorkflowAppUri}/coordinator.xml</value>
		</property>
		<property>
			<name>condenseCoord</name>
			<value>${condenseWorkflowAppUri}/coordinator.xml</value>
		</property>
		<property>
			<name>caCoord</name>
			<value>${caWorkflowAppUri}/coordinator.xml</value>
		</property>
		<property>
			<name>caCondenseCoord</name>
			<value>${caCondenseWorkflowAppUri}/coordinator.xml</value>
		</property>
		<property>
			<name>claimsCoord</name>
			<value>${claimsWorkflowAppUri}/coordinator.xml</value>
		</property>
		<property>
			<name>claimsCondenseCoord</name>
			<value>${claimsCondenseWorkflowAppUri}/coordinator.xml</value>
		</property>
		<property>
			<name>inspctCoord</name>
			<value>${inspctWorkflowAppUri}/coordinator.xml</value>
		</property>
		<property>
			<name>inspctCondenseCoord</name>
			<value>${inspctCondenseWorkflowAppUri}/coordinator.xml</value>
		</property>
		<property>
			<name>avesCoord</name>
			<value>${avesWorkflowAppUri}/coordinator.xml</value>
		</property>
		<property>
			<name>avesCondenseCoord</name>
			<value>${avesCondenseWorkflowAppUri}/coordinator.xml</value>
		</property>
	</parameters>
	<controls>
		<kick-off-time>${startDate}</kick-off-time>
	</controls>
	<coordinator name='EQUIP-Techline-Ingest-Daily-${env}-coord'>
		<app-path>${techlineWorkflowAppUri}</app-path>
		<configuration>
			<property>
				<name>workflowAppUri</name>
				<value>${techlineWorkflowAppUri}</value>
			</property>
			<property>
				<name>collectionName</name>
				<value>${techlineCollectionName}</value>
			</property>
		</configuration>
	</coordinator>
	<coordinator name='EQUIP-Techline-Condense-Weekly-${env}-coord'>
		<app-path>${condenseCoord}</app-path>
		<configuration>
			<property>
				<name>workflowAppUri</name>
				<value>${condenseWorkflowAppUri}</value>
			</property>
		</configuration>
	</coordinator>
	<coordinator name='EQUIP-ConsumerAffairs-Ingest-Daily-${env}-coord'>
		<app-path>${caWorkflowAppUri}</app-path>
		<configuration>
			<property>
				<name>workflowAppUri</name>
				<value>${caWorkflowAppUri}</value>
			</property>
			<property>
				<name>collectionName</name>
				<value>${caCollectionName}</value>
			</property>
		</configuration>
	</coordinator>
	<coordinator name='EQUIP-ConsumerAffairs-Condense-Weekly-${env}-coord'>
		<app-path>${caCondenseWorkflowAppUri}</app-path>
		<configuration>
			<property>
				<name>workflowAppUri</name>
				<value>${caCondenseWorkflowAppUri}</value>
			</property>
		</configuration>
	</coordinator>
	<coordinator name='EQUIP-Claims-Solr-Daily-${env}-coord'>
		<app-path>${claimsWorkflowAppUri}</app-path>
		<configuration>
			<property>
				<name>workflowAppUri</name>
				<value>${claimsWorkflowAppUri}</value>
			</property>
			<property>
				<name>collectionName</name>
				<value>${claimsCollectionName}</value>
			</property>
			<property>
				<name>legacyCollectionName</name>
				<value>${claimsLegacyCollectionName}</value>
			</property>
		</configuration>
	</coordinator>
	<coordinator name='EQUIP-Claims-Condense-Daily-${env}-coord'>
		<app-path>${claimsCondenseWorkflowAppUri}</app-path>
		<configuration>
			<property>
				<name>workflowAppUri</name>
				<value>${claimsCondenseWorkflowAppUri}</value>
			</property>
		</configuration>
	</coordinator>
	<coordinator name='EQUIP-INSPECT-Ingest-Daily-${env}-coord'>
		<app-path>${inspctWorkflowAppUri}</app-path>
		<configuration>
			<property>
				<name>workflowAppUri</name>
				<value>${inspctWorkflowAppUri}</value>
			</property>
			<property>
				<name>collectionName</name>
				<value>${inspctCollectionName}</value>
			</property>
		</configuration>
	</coordinator>
	<coordinator name='EQUIP-INSPECT-Condense-Weekly-${env}-coord'>
		<app-path>${inspctCondenseWorkflowAppUri}</app-path>
		<configuration>
			<property>
				<name>workflowAppUri</name>
				<value>${inspctCondenseWorkflowAppUri}</value>
			</property>
		</configuration>
	</coordinator>
	<coordinator name='EQUIP-AVES-Ingest-Daily-${env}-coord'>
		<app-path>${avesWorkflowAppUri}</app-path>
		<configuration>
			<property>
				<name>workflowAppUri</name>
				<value>${avesWorkflowAppUri}</value>
			</property>
			<property>
				<name>collectionName</name>
				<value>${avesCollectionName}</value>
			</property>
		</configuration>
	</coordinator>
	<coordinator name='EQUIP-AVES-Condense-Weekly-${env}-coord'>
		<app-path>${avesCondenseWorkflowAppUri}</app-path>
		<configuration>
			<property>
				<name>workflowAppUri</name>
				<value>${avesCondenseWorkflowAppUri}</value>
			</property>
		</configuration>
	</coordinator>
</bundle-app>
